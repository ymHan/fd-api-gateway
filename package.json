
{
  "name": "fd-nestjs-api-gateway",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run prebuild && NODE_ENV=dev nest start --watch",
    "start:debug": "npm run prebuild && NODE_ENV=dev nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "proto:install": "npm i git+https://github.com/ymHan/fd-proto.git",
    "proto:member": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/fd-proto/proto --ts_proto_out=src/proto/ node_modules/fd-proto/proto/member.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:backoffice": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/fd-proto/proto --ts_proto_out=src/proto/ node_modules/fd-proto/proto/backoffice.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:fdition": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/fd-proto/proto --ts_proto_out=src/proto/ node_modules/fd-proto/proto/fdition.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:fdist": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/fd-proto/proto --ts_proto_out=src/proto/ node_modules/fd-proto/proto/fdist.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto": "npm run proto:install && npm run proto:member && npm run proto:backoffice && npm run proto:fdition && npm run proto:fdist"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.4",
    "@grpc/proto-loader": "^0.7.11",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.5",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.5",
    "@nestjs/microservices": "^10.3.5",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/platform-socket.io": "^10.3.5",
    "@nestjs/platform-ws": "^10.3.5",
    "@nestjs/swagger": "^7.3.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.6.8",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ioredis": "^5.3.2",
    "nestjs-real-ip": "^3.0.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "request-ip": "^3.3.0",
    "request-promise": "^4.2.6",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "socket.io-redis": "^6.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fd-proto": "github:ymHan/fd-proto",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.170.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
